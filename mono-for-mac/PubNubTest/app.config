<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="pubnubListener"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="pubnub-messaging.log"/>
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
  <system.net>
    <connectionManagement>
      <add address="*" maxconnection="600" />
    </connectionManagement>
  </system.net>   
</configuration>